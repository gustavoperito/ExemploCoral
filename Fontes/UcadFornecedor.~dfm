  object SqlFor: TSQLDataSet
    CommandText = 
      'select '#13#10'FORNECEDORES.*,'#13#10'ENDERECOS.CEP,'#13#10'CIDADES.NOME_CID,'#13#10'BAI' +
      'RROS.NOME_BAI'#13#10' '#13#10'from FORNECEDORES, ENDERECOS, CIDADES, BAIRROS' +
      #13#10#13#10'WHERE '#13#10'FORNECEDORES.IDFORNECEDOR = :PARIDFORNECEDOR '#13#10'AND F' +
      'ORNECEDORES.IDENDERECO=ENDERECOS.IDENDERECO AND ENDERECOS.IDBAIR' +
      'RO = BAIRROS.IDBAIRRO'#13#10'AND ENDERECOS.IDCIDADE = CIDADES.IDCIDADE'
    DataSource = DsFor
    MaxBlobSize = -1
    Params = <
      item
        DataType = ftString
        Name = 'PARIDFORNECEDOR'
        ParamType = ptInput
        Value = '0'
      end>
    SQLConnection = DM.conexao
    Left = 48
    Top = 8
    object SqlForIDFORNECEDOR: TIntegerField
